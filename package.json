{
  "name": "reactjs-boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.3.2",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/styles": "^4.3.0",
    "connected-react-router": "^6.5.2",
    "formik": "^1.5.8",
    "history": "^4.9.0",
    "lodash": "^4.17.15",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "redux-act": "^1.7.7",
    "redux-persist": "^5.10.0",
    "redux-saga": "^1.0.5",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "docz": "^0.12.5",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^3.0.3",
    "jest-dom": "^3.5.0",
    "lint-staged": "^8.0.3",
    "prettier-eslint-cli": "^4.7.1",
    "react-testing-library": "^7.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint": "eslint src",
    "eject": "react-scripts eject",
    "pre-commit": "eslint src",
    "format": "prettier-eslint 'src/**/*.js' --write --no-semi --single-quote"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
